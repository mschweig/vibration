&spi1 {
	compatible = "nordic,nrf-spim";
	status = "okay";
	sck-pin = <47>;
	mosi-pin = <45>;
	miso-pin = <46>;
	cs-gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;

	st25r3911b@0 {
		label = "ST25R3911B";
		compatible = "st,st25r3911b";
		reg = <0>;
		spi-max-frequency = <4000000>;
		irq-gpios = <&gpio0 4 0>;
		led-nfca-gpios = <&gpio0 7 0>;
	};
};
